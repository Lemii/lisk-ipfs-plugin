    "@types/express-rate-limit": "^5.1.1",
    "@types/jest": "^26.0.20",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/node": "^12.12.11",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
